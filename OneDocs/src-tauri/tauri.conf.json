{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "onedocs",
  "version": "0.1.0",
  "identifier": "com.ijune.onedocs",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "一文亦闻",
        "width": 800,
        "height": 700,
        "resizable": false,
        "label": "main"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}